
@{
    ViewBag.Title = "Index";
}

<div class="col-md-3">
    @{ Html.RenderAction("SideMenu", "Test");}
</div>
<div class="col-md-3">
    <h2>@ViewBag.EmpName </h2>

    <table class="table table-striped">
        <thead>
            <tr>

                <th>Name</th>
                <th>Department Name</th>
                <th>Address</th>
            </tr>
        </thead>
        
        <tbody>
            @if (ViewBag.EmployeeList != null)
            {
                foreach (var item in ViewBag.EmployeeList)
                {
            <tr>

                <td>@item.Name</td>
                <td>@item.DepartmentName</td>
                <td>@item.Address</td>
            </tr>
                }
            }
        </tbody>
    </table>

   
</div>



